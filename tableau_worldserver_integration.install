<?php

/**
 * @file
 * Install and update hooks for the Tableau WorldServer Integration module.
 */


/**
 * Implements hook_uninstall().
 */
function tableau_worldserver_integration_uninstall() {
  $vars = array(
    'tableau_worldserver_integration_sftp_host',
    'tableau_worldserver_integration_sftp_un',
    'tableau_worldserver_integration_sftp_pw',
    'tableau_worldserver_integration_target_root',
    'tableau_worldserver_integration_source_root',
  );

  foreach ($vars as $var) {
    variable_del($var);
  }
}
